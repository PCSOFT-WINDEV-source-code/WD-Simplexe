        internal_properties : CAAAAAgAAAA5eyxnX4yNxlHaNzhkb65/eYsO/FY3J7Oh2PM+eFVJ0UtegKCNKt2RUpnv5vRKQ7xJvDjk5grlOUHQ++yBXyvHboanyhqXRwmr5RWD7zkNlRQO197zKjlYG7vIIwBoyDMybjL4l304nFJkhd4=
        code : |1-
         // Variables
         aaConstraint is array of TABLE_Simplexe..NumberColumn-1 real // Don't count the "Variable" column that is hidden
         aaCoeff      is array of TABLE_Simplexe..NumberColumn-2 real // Remove the "Total" column and the "Variable" column that is hidden
         sString      is string
         rZ           is real
         counter     is int
         sResult    is string
         
         // If rows have been inserted, try to calculate
         IF TableCount(TABLE_Simplexe,toTotal) > 0 THEN
         	
         	// Initialize the Simplexe
         	Initialize()
         	
         	// Add the variables
         	FOR I = 1 _TO_ TABLE_Simplexe..NumberColumn-2 // Remove the "Total" column and the "Variable" column that is hidden
         		AddVariable("x"+I)
         		
         		IF EncounteredError() THEN
         			Error(InformationError())
         			RETURN
         		END
         	END
         	
         	// Add the constraints by filling an array
         	FOR line = 1 _TO_ TableCount(TABLE_Simplexe,toTotal)-1 
         		FOR column = 2 _TO_ TABLE_Simplexe..NumberColumn
         			aaConstraint[column-1] = TABLE_Simplexe[line][column] 
         		END
         		AddConstraintArray(aaConstraint)
         		
         		IF EncounteredError() THEN
         			Error(InformationError())
         			RETURN
         		END
         	END
         	
         	// Fill the array of coefficients for the economical function
         	FOR column = 2 _TO_ TABLE_Simplexe..NumberColumn-1 // Remove the "Total" column
         		aaCoeff[column-1] = TABLE_Simplexe[TableCount(TABLE_Simplexe,toTotal)][column] 
         	END
         	
         	// Maximize
         	sString = MaximizeSimplexeArray(aaCoeff)
         	
         	IF EncounteredError() THEN
         		Error(InformationError())
         		RETURN
         	ELSE
         		// Divide the returned string that has the following format: |Z|nbX1|nameX1|nbX2|nameX2|etc..
         		rZ = Val(ExtractString(sString,1,"|"))
         		sResult = "Z = "+rZ + " = "
         		
         		// Analyze the rest of the string returned to display the results properly
         		counter = 2
         		WHILE counter <= StringCount(sString,"|")
         			sResult += " +"+ExtractString(sString,counter,"|")+"*"+ExtractString(sString,counter+1,"|")
         			counter+=2
         		END
         		
         		// Display the result
         		Info(sResult)
         	END
         ELSE
         	// Information
         	Info(<ยง$0001ยง>)	
         END
        type : 18
